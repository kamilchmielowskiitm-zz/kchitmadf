{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kchitmadf"
		},
		"Azurekchitm_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "kchitmdb"
		},
		"kchitmdl2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kchdl22.dfs.core.windows.net"
		},
		"kchitmkv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dkchitmkv.vault.azure.net/"
		},
		"kchitmlocaldb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "kchdblocal"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"kchitmkv_properties_typeProperties_baseUrl": {
						"value": "[parameters('kchitmkv_properties_typeProperties_baseUrl')]"
					},
					"Azurekchitm_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('Azurekchitm_properties_typeProperties_connectionString_secretName')]"
					},
					"kchitmdl2_properties_typeProperties_url": {
						"value": "[parameters('kchitmdl2_properties_typeProperties_url')]"
					},
					"kchitmlocaldb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('kchitmlocaldb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}