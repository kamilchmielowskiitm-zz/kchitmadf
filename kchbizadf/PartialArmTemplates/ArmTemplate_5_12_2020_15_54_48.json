					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "DimDate.csv",
						"fileSystem": "raw"
					},
					"columnDelimiter": ";",
					"escapeChar": "\\",
					"quoteChar": "\""
				},
				"schema": [
					{
